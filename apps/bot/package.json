    "@discordjs/core": "^2.4.0",
    "@discordjs/rest": "^2.6.0",
    "@discordjs/ws": "^2.0.4",
    "@elysiajs/eden": "^1.4.5",
    "@sapphire/discord.js-utilities": "^7.3.3",
